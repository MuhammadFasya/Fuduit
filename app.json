{
  "expo": {
    "name": "Fuduit",
    "slug": "fuduit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E14"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fuduit.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0E14"
      },
      "package": "com.fuduit.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [["expo-sqlite"]]
  }
}
