{
  "expo": {
    "name": "Fuduit",
    "slug": "fuduit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0A0E14"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fuduit.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A0E14"
      },
      "package": "com.fuduit.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [["expo-sqlite"]]
  }
}
